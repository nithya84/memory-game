{
  "name": "memory-game-backend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.377.0",
    "@aws-sdk/client-dynamodb": "^3.377.0",
    "@aws-sdk/client-s3": "^3.835.0",
    "@aws-sdk/lib-dynamodb": "^3.377.0",
    "aws-lambda": "^1.0.7",
    "joi": "^17.9.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "aws-sdk-client-mock": "^3.0.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "serverless": "^3.33.0",
    "serverless-offline": "^13.9.0",
    "serverless-webpack": "^5.13.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "webpack": "^5.88.2"
  },
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && serverless offline start --stage local",
    "dev:admin": "npm run build && serverless offline start --stage local --config serverless-admin.yml",
    "package": "serverless package",
    "package:admin": "serverless package --config serverless-admin.yml",
    "deploy": "serverless deploy",
    "deploy:admin": "serverless deploy --config serverless-admin.yml",
    "remove:admin": "serverless remove --config serverless-admin.yml",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf .serverless node_modules dist .webpack"
  }
}
